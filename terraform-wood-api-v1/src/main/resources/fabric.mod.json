    "fabric-rendering-v1": "*",
    "fabric-resource-loader-v0": "*"
  },
  "authors": [
    "Prospector",
    "coderbot16",
    "TerraformersMC"
  ],
  "contributors": [
    "SuperCoder79",
    "NeusFear",
    "Vaerian",
    "NebelNidas",
    "gniftygnome"
